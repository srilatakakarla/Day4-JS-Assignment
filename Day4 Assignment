     <script>
       function myFunction() 
       {
        myFunction()
        {
            var i = 1; i <= i; i++ )
                {
                    if ( i%3 === 0 && i%5 === 0 )
                        {
                            console.log( i + " FizzBuzz" );
                        }
                    else if ( i%3 === 0 ) 
                        {
                            console.log(i+ " Fizz" );
                        }
                            else if ( i%5 === 0 ) 
                                {
                                    console.log(i+ " Buzz" );
                                }
                            else
                                {
                                    console.log(i);
                                }
                        }
       }        
      </script>
